import java.util.Scanner;

public class Sum{
	public static void main (String [] args ){
		Scanner input = new Scanner(System.in);
		
		int num ;
		int i = 1;
		int sum = 0 ;
		
		while (i <= 10){
			System.out.printf("Enter num%d %n: " , 1);
			num = input .nextInt();
			
			sum +=  num;
			i++;
			
	}
	}
}
